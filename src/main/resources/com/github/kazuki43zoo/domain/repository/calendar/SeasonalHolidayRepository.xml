<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.kazuki43zoo.domain.repository.calendar.SeasonalHolidayRepository">

    <select id="findAll" resultType="SeasonalHoliday">
        SELECT
        target_date
        ,holiday_name
        ,holiday_name_ja
        FROM
        seasonal_holiday
        ORDER BY
        target_date
    </select>

</mapper>