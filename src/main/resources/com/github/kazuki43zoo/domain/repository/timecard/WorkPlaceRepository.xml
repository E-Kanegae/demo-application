<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.kazuki43zoo.domain.repository.timecard.WorkPlaceRepository">
    <resultMap id="resultMap-WorkPlace" type="WorkPlace">
        <id property="workPlaceUuid" column="work_place_uuid" />
    </resultMap>

    <select id="findOne" resultMap="resultMap-WorkPlace">
        SELECT
            work_place_uuid
            ,work_place_name
        FROM
            work_place
        WHERE
            work_place_uuid = #{workPlaceUuid}
    </select>

</mapper>